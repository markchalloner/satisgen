{
    "name": "markchalloner/satisgen",
    "description": "Generates a Satis satis.json file from a satis.php file",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Challoner",
            "email": "mark.a.r.challoner@gmail.com"
        }
    ],
    "bin": ["bin/satisgen"],
    "require": {
        "php": ">=5.3.0",
        "symfony/console": "~2.1",
        "symfony/filesystem": "~2.6",
        "vlucas/phpdotenv": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "phpunit/php-code-coverage": "~2.0",
        "mikey179/vfsStream": "~1.5"
    },
    "autoload": {
        "psr-4": { "SatisGen\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "SatisGen\\Tests\\": "tests/" }
    }
}
