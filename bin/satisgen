#!/usr/bin/env php
<?php

require __DIR__.'/../src/bootstrap.php';

use SatisGen\Application\GenerateApplication;
use SatisGen\Command\GenerateCommand;
use SatisGen\Config\ConfigInputReader;
use Symfony\Component\Console\Application;
